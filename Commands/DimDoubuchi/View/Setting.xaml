<Window x:Class="BimIshou.Commands.DimDoubuchi.View.Setting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BimIshou.Commands.DimDoubuchi.View"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="Setting" Height="110" Width="200">
    <Grid>
        <StackPanel>
            <TextBox VerticalContentAlignment="Center" Margin="3" x:Name="settingDim" Height="30"/>
            <DockPanel Height="30" HorizontalAlignment="Right" LastChildFill="False">
                <Button Margin="5" Width="60" Content="OK" Click="Button_Click"/>
            </DockPanel>
        </StackPanel>
    </Grid>
</Window>
